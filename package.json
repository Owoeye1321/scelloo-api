{
  "name": "Scelloo-api-service",
  "version": "1.0.0",
  "description": "Scelloo-api System Service",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm run nodemon",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config build/index.js",
    "build": "tsc",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config --exec ts-node --files src/index.ts",
    "test": "export APP_ENV=test; jest",
    "test:watch": "export APP_ENV=test; jest --watchAll",
    "coverage": "export APP_ENV=test; jest --coverage",
    "coverage:coveralls": "export APP_ENV=test; jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e": "export APP_ENV=test; jest -c jest.integration.config.js",
    "test:e2e:watch": "export APP_ENV=test; jest -c jest.integration.config.js --watchAll",
    "test:e2e:coverage": "export APP_ENV=test; jest -c jest.integration.config.js --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongoose",
    "typescript",
    "boilerplate"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.175",
    "@types/morgan": "^1.9.2",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "coveralls": "^3.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "nanoid": "^3.1.25",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sequelize": "^6.37.5",
    "ts-node": "^10.9.1",
    "winston": "^3.3.3"
  }
}
